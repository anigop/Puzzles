
public class RobotMovements {

	static int[][] grid;
	public static int move(int x , int y){
		
		if(x == 0 && y == 0){
			return 1;
		}
	}
	
	public static boolean validPos(int x , int y){
		return x > 0 && y > 0 && grid[x][y] == 0;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		grid = new int[4][4];
	}

}
