import java.io.BufferedReader;
import java.io.FileReader;
import java.util.ArrayList;


public class UglyNumbers {

	static ArrayList<String> sequences = new ArrayList<String>();
public static void readFromFile(String filename){
		
		try{
			BufferedReader br = new BufferedReader(new FileReader(filename));
			String line = null;
			
			while((line = br.readLine()) != null){
				countUgly(line,"");
			}
		}catch(Exception e){
			e.printStackTrace();
		}
	}
	

	public static void countUgly(String line, String sequence){
		
		if(line.length() == 0){
			sequences.add(sequence);
			return;
		}
		
		sequence += line.substring(0,1)+"+";
		
		countUgly(line.substring(1), sequence);
		
	}
	
	public static void sequences(){
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "Hello";
		
		System.out.println(s.substring(0, 1));
	}

}
