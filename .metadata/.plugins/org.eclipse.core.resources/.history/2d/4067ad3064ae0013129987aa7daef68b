import java.io.BufferedReader;

public class FileRead {

	public void readFromFile(String filename){
		
		try{

			
			BufferedReader br = new BufferedReader(new FileRead(filename));
			String line = null;
			String jsonString = "";
			
			while((line = br.readLine()) != null){
				jsonString += line;
			}
			
			
		}catch(Exception e){
			e.printStackTrace();
		}
	}
	
}
