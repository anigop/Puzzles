import java.io.BufferedReader;
import java.io.FileReader;
import java.util.ArrayList;
import java.util.Stack;


public class UglyNumbers {

	static ArrayList<String> sequences = new ArrayList<String>();
public static void readFromFile(String filename){
		
		try{
			BufferedReader br = new BufferedReader(new FileReader(filename));
			String line = null;
			
			while((line = br.readLine()) != null){
				countUgly(line,"");
			}
		}catch(Exception e){
			e.printStackTrace();
		}
	}
	

	public static void countUgly(String line, String sequence){
		
		if(line.length() == 1){
			sequences.add(sequence+line);
			return;
		}
		
		
		
		countUgly(line.substring(1), sequence+line.substring(0,1)+"+");
		countUgly(line.substring(1), sequence+line.substring(0,1)+"-");
		countUgly(line.substring(1), sequence+line.substring(0,1));
		
		
	}
	
	public static void evalseq(String seq){
		
		Stack<String> operator = new Stack<String>();
		Stack<String> operand = new Stack<String>();
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "Hello";
		
		System.out.println(s.substring(1));
		
		countUgly("12345", "");
		
		int val =  (int) Math.pow(3.0, 4.0);
		
		
	}

}
